-- SetupConfig: { "AddRequires": "Model.CK.UserPasswordTable" } 
-- 
create transformer on CK.vUserAuthProvider
as
begin
	inject "
	union all
	select UserId, 'Basic', case LastLoginTime when '0001-01-01 00:00:00' then null else LastLoginTime end from CK.tUserPassword where UserId > 0
	" after first part {select};
end