<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata >
    <id>CK.DB.Zone.SimpleNaming</id>
    <version>$version$</version>
    <authors>Invenietis</authors>
    <owners>Invenietis</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <summary>
      CK-Database that constrains group names (from CK.DB.Group.SimpleNaming) to be unique in Zone ({{CSemVer}}).
    </summary>
    <description>
      CK.tGroup.GrouName column from CK.DB.Group.SimpleNaming is globally unique. This package makes it
      unique in the scope of the group's Zone (from CK.DB.Zone).
    </description>
    <language>en-US</language>
    <dependencies>
      <dependency id="CK.DB.Zone" version="$version$" />
      <dependency id="CK.DB.Group.SimpleNaming" version="$version$" />
    </dependencies>
  </metadata>
  <files>    
    <file src="CK.DB.Zone.SimpleNaming\bin\{{configuration}}\CK.DB.Zone.SimpleNaming.dll" target="lib\net45" />
    <file src="CK.DB.Zone.SimpleNaming\bin\{{configuration}}\CK.DB.Zone.SimpleNaming.xml" target="lib\net45" />
  </files>
</package>